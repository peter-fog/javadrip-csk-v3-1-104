    "build:next": "next build",
    "start": "next start",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "uniform:pull:manifest": "uniform context manifest download --output ./src/context/manifest.json",
    "uniform:pull:datatype": "uniform canvas datatype pull ./content/full-demo.json",
