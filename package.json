    "uniform:pull:context:quirks": "uniform context quirk pull content/full-demo.json",
    "uniform:pull:redirect": "uniform redirect definition pull ./content/full-demo.json",
    "uniform:pull:context": "run-s uniform:pull:context:signals uniform:pull:context:enrichments",
