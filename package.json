    "uniform:push:datatype": "uniform canvas datatype push ./content/full-demo.json",
    "uniform:push:components": "uniform canvas component push ./content/full-demo.json",
    "uniform:push:compositions": "uniform canvas composition push ./content/full-demo.json",
    "uniform:push:project-map-definitions": "uniform project-map definition push ./content/full-demo.json",
    "uniform:push:project-map-nodes": "uniform project-map node push ./content/full-demo.json",
    "uniform:push:context:signals": "uniform context signal push ./content/full-demo.json",
    "uniform:push:context:enrichments": "uniform context enrichment push ./content/full-demo.json",
    "uniform:push:context:intents": "uniform context intent push content/full-demo.json",
    "uniform:push:context:audiences": "uniform context audience push content/full-demo.json",
    "uniform:push:context:quirks": "uniform context quirk push content/full-demo.json",
    "uniform:push:redirect": "uniform redirect definition push ./content/full-demo.json",
    "uniform:push:context": "run-s uniform:push:context:signals uniform:push:context:enrichments",
    "uniform:push:context:full": "run-s uniform:push:context:signals uniform:push:context:enrichments uniform:push:context:quirks uniform:push:context:audiences uniform:push:context:intents",
    "uniform:push": "run-s uniform:push:datatype uniform:push:components uniform:push:compositions uniform:push:project-map-definitions uniform:push:project-map-nodes uniform:push:redirect uniform:push:context:full",
    "uniform:composition:publish": "uniform canvas composition push ./content/full-demo.json --state published",
    "uniform:manifest:publish": "uniform context manifest publish",
    "uniform:publish": "run-s uniform:composition:publish uniform:manifest:publish",
    "import:products": "ts-node src/scripts/import-products",
    "import:articles": "ts-node src/scripts/import-articles",
    "uniform:get:redirects": "ts-node src/scripts/get-redirects.ts",
    "export:contentful-articles": "ts-node src/scripts/export-contentful-articles",
    "postinstall": "cd cli-run && npm install && npm run build",
    "cli": "cd cli-run && node dist/index.mjs "
